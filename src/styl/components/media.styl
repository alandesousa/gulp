//@media only screen and (min-width 992px) and (max-width 1200px)
.hidden-lg
	display none !important

@media only screen and (min-width 992px) and (max-width 1200px)
	.hidden-md
		display none !important
	.container
		width 100%
			
@media only screen and (min-width 768px) and (max-width 991px)
	.hidden-sm
		display none !important
	.container
		width 100%


@media only screen and  (max-width 767px)
	.hidden-xs
		display none !important
	.hidden-lg
		display block !important
	.container
		width 100%
